
@{
    Layout = null;
}

<!DOCTYPE html>

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>HRVacation</title>
    <link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@300;400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/astromotion/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/astromotion/css/slick.css" type="text/css" />
    <link rel="stylesheet" href="~/astromotion/css/templatemo-style.css">
    <!--
    @RenderSection("hrvacationstyle", required: false)

    TemplateMo 560 Astro Motion

    https://templatemo.com/tm-560-astro-motion

    -->
</head>
<body>
    <video autoplay muted loop id="bg-video">
        <source src="~/astromotion/video/gfp-astro-timelapse.mp4" type="video/mp4">
    </video>
    <div class="page-container">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-12">

                    <p class="aligncenter margintop20">
                        <a href="#mySignin" data-dismiss="modal" aria-hidden="true" data-toggle="modal">Kayıt Ol</a>
                    </p>
                    <p class="aligncenter margintop20">
                        <a href="#mySignin" data-dismiss="modal" aria-hidden="true" data-toggle="modal">Giriş Yap</a>
                    </p>

                    <div class="cd-slider-nav">
                        <nav class="navbar navbar-expand-lg" id="tm-nav">
                            <a class="navbar-brand" href="#">HumanGroup</a>
                            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-supported-content" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"></span>
                            </button>
                            <div class="collapse navbar-collapse" id="navbar-supported-content">
                                <ul class="navbar-nav mb-2 mb-lg-0">
                                    <li class="nav-item selected">
                                        <a class="nav-link" aria-current="page" href="#0" data-no="1">GİRİŞ</a>
                                        <div class="circle"></div>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#0" data-no="2">GALERİ</a>
                                        <div class="circle"></div>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#0" data-no="3">HAKKIMIZDA</a>
                                        <div class="circle"></div>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#0" data-no="4">İLETİŞİM</a>
                                        <div class="circle"></div>
                                    </li>
                                </ul>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid tm-content-container">
            <ul class="cd-hero-slider mb-0 py-7">
                @RenderBody()
                <li class="px-5" data-page-no="1">
                    <div class="page-width-5 page-left">
                        <div class="d-flex position-relative tm-border-top tm-border-bottom intro-container">
                            <div class="intro-left tm-bg-dark">
                                <h2 class="mb-4">Human Group'a Hoş Geldiniz</h2>
                                <p class="mb-4">
                                    HumanGroup olarak faaliyetlerimizin temelini iş hayatında mutluluğu artırmak oluşturuyor. Mutlu yöneticilerin, çalışma arkadaşlarına, ekiplerine ve şirket iklimine olumlu duygular kattığına inanıyoruz.Türkiye’nin alanında en yetkin, çok disiplinli ve deneyimli danışmanlarından oluşan ekibimizle “iş’te mutluluk” için çalışıyorız.
                                </p>
                                <p class="mb-0">
                                    Misyonumuz, şirketinizi sürdürülebilir başarıya taşıyacak güçlü ve etkili bir çalışan deneyimi tasarlamanıza yardımcı olmak.
                                    Vizyonumuz, ulusal bir firma olarak sunduğumuz hizmet kalitesiyle uluslararası alanda da tercih edilen şirket olabilmek.
                                </p>
                            </div>
                            <div class="intro-right">
                                <img src="~/astromotion/img/home-img-3.jpg" alt="Image" class="img-fluid intro-img-1">
                            </div>
                            <div class="circle intro-circle-1"></div>
                            <div class="circle intro-circle-2"></div>
                            <div class="circle intro-circle-3"></div>
                            <div class="circle intro-circle-4"></div>
                        </div>
                        <div class="text-center">
                            <a href="#0" data-page-no="2" class="btn btn-primary tm-intro-btn tm-page-link">
                                Galeriyi Görüntüle
                            </a>
                        </div>
                    </div>
                </li>
                <li data-page-no="2">
                    <!-- Image Carousel -->
                    <div class="mx-auto position-relative gallery-container">
                        <div class="circle intro-circle-1"></div>
                        <div class="circle intro-circle-2"></div>
                        <div class="mx-auto tm-border-top gallery-slider">
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-1.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>Liderlikte Onur Sözü'nü çalışıyoruz'</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-2.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>İşimiz insan,IK gönül verdik biz...</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-3.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>Toplantıdan</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-4.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>HumanGroup Seminerde</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-5.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>HumanGroup Haliç Konferansında</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-6.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>Mülakat Teknikleri</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
                                <img src="~/astromotion/img/gallery-img-7.jpg" alt="Image">
                                <figcaption>
                                    <div>
                                        <p>Toplantıda</p>
                                    </div>
                                    <a href="#">View more</a>
                                </figcaption>
                            </figure>
                            <figure class="effect-julia item">
